{
  "name": "rsv360-ecosystem",
  "version": "1.0.0",
  "description": "RSV360 Ecosystem - 39 Microservices Platform",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:performance": "artillery run artillery/load-test.yml",
    "test:security": "npm audit && snyk test",
    "test:accessibility": "cypress run --spec 'cypress/e2e/accessibility.cy.ts'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t rsv360-ecosystem .",
    "docker:run": "docker run -p 3000:3000 rsv360-ecosystem",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:guest": "cd apps/guest && next dev -p 3000",
    "dev:admin": "cd apps/admin && next dev -p 3001",
    "dev:api": "cd apps/api && npm run dev",
    "dev:jobs": "cd apps/jobs && npm run dev",
    "dev:migration": "concurrently \"npm run dev:guest\" \"npm run dev:admin\"",
    "dev:all-separated": "concurrently \"npm run dev:guest\" \"npm run dev:admin\" \"npm run dev:api\" \"npm run dev:jobs\""
  },
  "keywords": [
    "microservices",
    "travel",
    "booking",
    "ecosystem",
    "rsv360"
  ],
  "author": "RSV Team",
  "license": "MIT",
  "dependencies": {
    "@godaddy/terminus": "^4.7.2",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "opossum": "^8.2.3",
    "p-timeout": "^6.1.2",
    "pg": "^8.16.3",
    "prisma": "^5.7.1",
    "prom-client": "^15.1.0",
    "redis": "^4.6.12",
    "retry": "^0.13.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tailwind-merge": "^3.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "artillery": "^2.0.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.0",
    "next": "^15.5.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "snyk": "^1.1248.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  }
}
